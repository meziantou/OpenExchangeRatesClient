<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net461</TargetFrameworks>
    <Authors>Meziantou</Authors>
    <Company>Meziantou</Company>
    <Description>Open Exchange Rates API client for .NET.</Description>
    <PackageProjectUrl>https://github.com/meziantou/OpenExchangeRatesClient</PackageProjectUrl>
    <Version>1.0.3</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/meziantou/OpenExchangeRatesClient</RepositoryUrl>
    <PackageTags>OpenExchangeRates</PackageTags>
  </PropertyGroup>
  
  <ItemGroup Condition="'$(TargetFramework)'=='net461'">
    <Reference Include="System.Net.Http" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

</Project>
